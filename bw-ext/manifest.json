{
  "name": "BW Extension",
  "description": "Adds extended functionality to BloodWars game.",
  "version": "0.5",
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [ "https://*.bloodwars.interia.pl/*" ],
      "js": [
        "Extensions/String.js",
        "Extensions/NodeList.js",
        "Core.js",
        "Modules/Module.js",
        "Modules/Armory/AddSearch.js",
        "Modules/Armory/LoadAllItems.js",
        "Modules/Auction/AddPrValueColumn.js",
        "Modules/Auction/HighlightImportantAffixes.js",
        "Modules/Buildings/AddSummaryTable.js",
        "Modules/Rank/ShowPlayersEligibleForGettingEvolutionPoints.js",
        "Modules/TownShop/AutomaticallyOpenBargains.js",
        "Modules/TownShop/HighlightPerfectItems.js",
        "Extension.js",
        "Run.js"
      ]
    }
  ]
}